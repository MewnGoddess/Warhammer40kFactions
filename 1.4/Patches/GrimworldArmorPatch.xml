<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Grimworld 40,000 - Angels of Death</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationRemove">
					<xpath>Defs/PawnKindDef[@Name = "SpaceMarineBasePawn"]/apparelRequired</xpath>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/PawnKindDef[@Name = "SpaceMarineBasePawn"]</xpath>
					<value>
						<apparelRequired>
							<li>Apparel_BasicShirt</li>
							<li>Apparel_Pants</li>
						</apparelRequired>
					</value>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>Defs/PawnKindDef[@Name = "PrimarisMarineBasePawn"]/apparelRequired</xpath>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/PawnKindDef[defName = "BEWH_SpaceMarine"]</xpath>
					<value>
						<apparelRequired>
							<li>ODZ_40k_AS_UM_MkVIIArmor</li>
							<li>ODZ_40k_AS_FB_UM_MkVIIHelmet</li>
						</apparelRequired>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/PawnKindDef[defName = "BEWH_PrimarisSpaceMarine"]</xpath>
					<value>
						<apparelRequired>
							<li>ODZ_40k_AS_UM_MkXArmor</li>
							<li>ODZ_40k_AS_UM_MkXHelmet</li>
						</apparelRequired>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/PawnKindDef[defName = "BEWH_SpaceMarinePsyker"]</xpath>
					<value>
						<apparelRequired>
							<li>ODZ_40k_AS_UM_MkXArmor</li>
							<li>ODZ_40k_AS_SG_UM_MkXHelmet</li>
						</apparelRequired>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/PawnKindDef[defName = "BEWH_Custodes"]</xpath>
					<value>
						<apparelRequired>
							<li>ODZ_40k_AS_UM_GravisArmor</li>
							<li>ODZ_40k_AS_FB_UM_GravisHelmet</li>
						</apparelRequired>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/PawnKindDef[defName = "BEWH_LordCommander"]</xpath>
					<value>
						<apparelRequired>
							<li>ODZ_40k_AS_UM_GravisArmor</li>
							<li>ODZ_40k_AS_FB_UM_GravisHelmet</li>
						</apparelRequired>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>